{{>nav-block}}
        <div class="container">
            <div class="row">
              <div class="col m12" id="article area">
                 {{#each articles}}
                    <div class="row">
                        <div class="col s12 m6">
                            <div class="card blue-grey darken-1">
                                <div class="card-content white-text">
                                    <span class="card-title"> {{ title }}</span>
                                    <img src="{{ imageSrc }}">
                                    <p>{{ description }}
                                        
                                    </p>
                                </div>
                                <div class="card-action">
                                    <a href="{{link}}" data-id="{{ _id }}" target="_blank">Read the Article</a>
                                    <a class="waves-effect waves-light btn modal-trigger" data-id="{{ _id }}" data-target="modal1" id="open-notes"><i class="material-icons left">speaker_notes</i>Add Note</a>
                                    <a class="waves-effect waves-light btn" data-id="{{ _id }}" id="remove-saved"><i class="material-icons left">clear_all</i>Unsave Article</a>
                                </div>
                            </div>
                        </div>
                    </div>
                 {{/each}}
                    
                </div>
            </div>
        </div>
        {{> modal-block }}
        